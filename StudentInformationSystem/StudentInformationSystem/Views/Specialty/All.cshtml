@model IEnumerable<SpecialtyAllViewModel>

@{
    ViewData["Title"] = "List of Specialties";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var specialty in Model)
    {
        <tr>
            <td>@specialty.Name</td>
            <td>
                <a asp-controller="Specialty" asp-action="Details" asp-route-id="@specialty.Id" class="btn btn-primary">Details</a>
            </td>
        </tr>
    }
    </tbody>
</table>